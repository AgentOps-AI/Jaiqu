{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
      "event_name": {
        "type": "string",
        "description": "The title of the event"
      },
      "event_description": {
        "type": "string",
        "description": "Descriptive details of the event"
      },
      "location": {
        "type": "string",
        "description": "Where is the event"
      }
    },
    "required": [
      "event_name",
      "location"
      
    ]
  }